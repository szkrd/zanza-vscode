{
  "name": "zanza",
  "displayName": "Zanza",
  "description": "simple things I miss from vscode",
  "publisher": "szkrd",
  "repository": "https://github.com/szkrd/zanza-vscode",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:zanza.copyToBuffer",
    "onCommand:zanza.pasteFromBuffer1",
    "onCommand:zanza.pasteFromBuffer2",
    "onCommand:zanza.pasteFromBuffer3",
    "onCommand:zanza.pasteFromBuffer4",
    "onCommand:zanza.pasteFromBuffer5",
    "onCommand:zanza.showClipboardBuffer",
    "onCommand:zanza.transformToCamelCase",
    "onCommand:zanza.transformToCamelCase",
    "onCommand:zanza.transformToKebabCase",
    "onCommand:zanza.transformToPascalCase",
    "onCommand:zanza.transformToSnakeCase",
    "onCommand:zanza.transformToScreamingSnakeCase",
    "onCommand:zanza.transformToConstantCase",
    "onCommand:zanza.commentDown",
    "onCommand:zanza.saveBookmark1",
    "onCommand:zanza.loadBookmark1",
    "onCommand:zanza.saveBookmark2",
    "onCommand:zanza.loadBookmark2",
    "onCommand:zanza.saveBookmark3",
    "onCommand:zanza.loadBookmark3",
    "onCommand:zanza.saveBookmark4",
    "onCommand:zanza.loadBookmark4",
    "onCommand:zanza.saveBookmark5",
    "onCommand:zanza.loadBookmark5",
    "onCommand:zanza.saveBookmark6",
    "onCommand:zanza.loadBookmark6",
    "onCommand:zanza.saveBookmark7",
    "onCommand:zanza.loadBookmark7",
    "onCommand:zanza.saveBookmark8",
    "onCommand:zanza.loadBookmark8",
    "onCommand:zanza.saveBookmark9",
    "onCommand:zanza.loadBookmark9",
    "onCommand:zanza.saveBookmark0",
    "onCommand:zanza.loadBookmark0",
    "onCommand:zanza.loadBookmarkFromPicker"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "key": "alt+backspace",
        "command": "editor.action.deleteLines",
        "when": "textInputFocus && !editorReadonly"
      },
      {
        "key": "ctrl+alt+\\",
        "command": "workbench.action.editorLayoutSingle"
      },
      {
        "key": "shift+left",
        "command": "workbench.files.action.collapseExplorerFolders",
        "when": "explorerViewletFocus && explorerViewletVisible && !inputFocus"
      },
      {
        "key": "ctrl+alt+/",
        "when": "editorTextFocus && !editorReadonly",
        "command": "editor.action.blockComment"
      },
      {
        "key": "ctrl+c",
        "mac": "cmd+c",
        "when": "editorFocus",
        "command": "zanza.copyToBuffer"
      },
      {
        "key": "ctrl+shift+v 1",
        "mac": "cmd+shift+v 1",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.pasteFromBuffer1"
      },
      {
        "key": "ctrl+shift+v 2",
        "mac": "cmd+shift+v 2",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.pasteFromBuffer2"
      },
      {
        "key": "ctrl+shift+v 3",
        "mac": "cmd+shift+v 3",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.pasteFromBuffer3"
      },
      {
        "key": "ctrl+shift+v 4",
        "mac": "cmd+shift+v 4",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.pasteFromBuffer4"
      },
      {
        "key": "ctrl+shift+v 5",
        "mac": "cmd+shift+v 5",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.pasteFromBuffer5"
      },
      {
        "key": "ctrl+shift+v ctrl+shift+v",
        "mac": "cmd+shift+v cmd+shift+v",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.showClipboardBuffer"
      },
      {
        "key": "ctrl+shift+/",
        "mac": "cmd+shift+/",
        "when": "editorTextFocus && !editorReadonly",
        "command": "zanza.commentDown"
      },
      {
        "key": "alt+shift+d 1",
        "command": "zanza.saveBookmark1"
      },
      {
        "key": "alt+d 1",
        "command": "zanza.loadBookmark1"
      },
      {
        "key": "alt+shift+d 2",
        "command": "zanza.saveBookmark2"
      },
      {
        "key": "alt+d 2",
        "command": "zanza.loadBookmark2"
      },
      {
        "key": "alt+shift+d 3",
        "command": "zanza.saveBookmark3"
      },
      {
        "key": "alt+d 3",
        "command": "zanza.loadBookmark3"
      },
      {
        "key": "alt+shift+d 4",
        "command": "zanza.saveBookmark4"
      },
      {
        "key": "alt+d 4",
        "command": "zanza.loadBookmark4"
      },
      {
        "key": "alt+shift+d 5",
        "command": "zanza.saveBookmark5"
      },
      {
        "key": "alt+d 5",
        "command": "zanza.loadBookmark5"
      },
      {
        "key": "alt+shift+d 6",
        "command": "zanza.saveBookmark6"
      },
      {
        "key": "alt+d 6",
        "command": "zanza.loadBookmark6"
      },
      {
        "key": "alt+shift+d 7",
        "command": "zanza.saveBookmark7"
      },
      {
        "key": "alt+d 7",
        "command": "zanza.loadBookmark7"
      },
      {
        "key": "alt+shift+d 8",
        "command": "zanza.saveBookmark8"
      },
      {
        "key": "alt+d 8",
        "command": "zanza.loadBookmark8"
      },
      {
        "key": "alt+shift+d 9",
        "command": "zanza.saveBookmark9"
      },
      {
        "key": "alt+d 9",
        "command": "zanza.loadBookmark9"
      },
      {
        "key": "alt+shift+d 0",
        "command": "zanza.saveBookmark0"
      },
      {
        "key": "alt+d 0",
        "command": "zanza.loadBookmark0"
      },
      {
        "key": "alt+d alt+d",
        "command": "zanza.loadBookmarkFromPicker"
      }
    ],
    "commands": [
      {
        "command": "zanza.copyToBuffer",
        "title": "Copy to Buffer"
      },
      {
        "command": "zanza.pasteFromBuffer1",
        "title": "Paste from Buffer [1]"
      },
      {
        "command": "zanza.pasteFromBuffer2",
        "title": "Paste from Buffer [2]"
      },
      {
        "command": "zanza.pasteFromBuffer3",
        "title": "Paste from Buffer [3]"
      },
      {
        "command": "zanza.pasteFromBuffer4",
        "title": "Paste from Buffer [4]"
      },
      {
        "command": "zanza.pasteFromBuffer5",
        "title": "Paste from Buffer [5]"
      },
      {
        "command": "zanza.showClipboardBuffer",
        "title": "Show Clipboard Buffer"
      },
      {
        "command": "zanza.transformToCamelCase",
        "title": "Transform Selection to Camel Case"
      },
      {
        "command": "zanza.transformToCamelCase",
        "title": "Transform Selection to Camel Case"
      },
      {
        "command": "zanza.transformToKebabCase",
        "title": "Transform Selection to Kebab Case"
      },
      {
        "command": "zanza.transformToPascalCase",
        "title": "Transform Selection to Pascal Case"
      },
      {
        "command": "zanza.transformToSnakeCase",
        "title": "Transform Selection to Snake Case Proper"
      },
      {
        "command": "zanza.transformToScreamingSnakeCase",
        "title": "Transform Selection to Screaming Snake Case"
      },
      {
        "command": "zanza.transformToConstantCase",
        "title": "Transform Selection to Constant Case"
      },
      {
        "command": "zanza.commentDown",
        "title": "Toggle Line Comment, Move Cursor Down"
      },
      {
        "command": "zanza.saveBookmark1",
        "title": "Save Bookmark [1]"
      },
      {
        "command": "zanza.loadBookmark1",
        "title": "Load Bookmark [1]"
      },
      {
        "command": "zanza.saveBookmark2",
        "title": "Save Bookmark [2]"
      },
      {
        "command": "zanza.loadBookmark2",
        "title": "Load Bookmark [2]"
      },
      {
        "command": "zanza.saveBookmark3",
        "title": "Save Bookmark [3]"
      },
      {
        "command": "zanza.loadBookmark3",
        "title": "Load Bookmark [3]"
      },
      {
        "command": "zanza.saveBookmark4",
        "title": "Save Bookmark [4]"
      },
      {
        "command": "zanza.loadBookmark4",
        "title": "Load Bookmark [4]"
      },
      {
        "command": "zanza.saveBookmark5",
        "title": "Save Bookmark [5]"
      },
      {
        "command": "zanza.loadBookmark5",
        "title": "Load Bookmark [5]"
      },
      {
        "command": "zanza.saveBookmark6",
        "title": "Save Bookmark [6]"
      },
      {
        "command": "zanza.loadBookmark6",
        "title": "Load Bookmark [6]"
      },
      {
        "command": "zanza.saveBookmark7",
        "title": "Save Bookmark [7]"
      },
      {
        "command": "zanza.loadBookmark7",
        "title": "Load Bookmark [7]"
      },
      {
        "command": "zanza.saveBookmark8",
        "title": "Save Bookmark [8]"
      },
      {
        "command": "zanza.loadBookmark8",
        "title": "Load Bookmark [8]"
      },
      {
        "command": "zanza.saveBookmark9",
        "title": "Save Bookmark [9]"
      },
      {
        "command": "zanza.loadBookmark9",
        "title": "Load Bookmark [9]"
      },
      {
        "command": "zanza.saveBookmark0",
        "title": "Save Bookmark [0]"
      },
      {
        "command": "zanza.loadBookmark0",
        "title": "Load Bookmark [0]"
      },
      {
        "command": "zanza.loadBookmarkFromPicker",
        "title": "Load Bookmark"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "build": "npx vsce package -o build",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts",
    "format": "prettier -w src",
    "test": "prettier -c src && eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "prettier": "2.3.2",
    "ts-node": "10.2.1",
    "typescript": "^4.3.2",
    "vsce": "1.96.1",
    "vscode-test": "^1.5.2"
  }
}
